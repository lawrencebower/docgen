<beans:beans xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://www.springframework.org/schema/beans classpath:/org/springframework/beans/factory/xml/spring-beans-3.1.xsd">

    <beans:bean name="overlayPDFTestInput" class="java.lang.String">
        <beans:constructor-arg value="C:\code\output\overlay_example_input.pdf"/>
    </beans:bean>
    <beans:bean name="overlayPDFTestOutput" class="java.lang.String">
        <beans:constructor-arg value="C:\code\output\overlay_test_output.pdf"/>
    </beans:bean>
    <beans:bean name="overlayPDFTestOutputExample" class="java.lang.String">
        <beans:constructor-arg value="C:\code\output\overlay_test_output_example.pdf"/>
    </beans:bean>

    <beans:bean name="customPDFTestExample" class="java.lang.String">
        <beans:constructor-arg value="C:\code\output\custom_example.pdf"/>
    </beans:bean>
    <beans:bean name="customPDFTestOutput" class="java.lang.String">
        <beans:constructor-arg value="C:\code\output\custom_example_output.pdf"/>
    </beans:bean>

    <beans:bean name="fileUtilsExampleFile" class="java.lang.String">
        <beans:constructor-arg value="C:\code\output\example_file.pdf"/>
    </beans:bean>

</beans:beans>