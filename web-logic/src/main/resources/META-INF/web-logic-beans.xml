<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
             http://www.springframework.org/schema/beans classpath:/org/springframework/beans/factory/xml/spring-beans-3.1.xsd
             http://www.springframework.org/schema/context classpath:/org/springframework/context/config/spring-context-3.1.xsd">

    <bean name="customerSelectionCB"
          class="org.lawrencebower.docgen.web_logic.business.controler_business.customer_selection.CustomerSelectionCB"/>

    <bean name="productSelectionCB"
          class="org.lawrencebower.docgen.web_logic.business.controler_business.product_selection.ProductSelectionCB"/>

    <bean name="dataEntryCB"
          class="org.lawrencebower.docgen.web_logic.business.controler_business.data_entry.DataEntryCB"/>

    <bean name="viewableComponentFilter"
          class="org.lawrencebower.docgen.web_logic.business.controler_business.data_entry.ViewableComponentFilter"/>

    <bean name="xmlModelMapper"
          class="org.lawrencebower.docgen.web_logic.business.model_factory.ModelFactoryCodeImpl"
          init-method="init"/>

    <bean name="customerProductMappings"
          class="org.lawrencebower.docgen.web_logic.business.mapping.CustomerProduct_Document_Mappings"/>

    <bean name="fieldMapper"
          class="org.lawrencebower.docgen.web_logic.business.mapping.FieldMapper"/>

    <bean name="reservedFieldMapper"
          class="org.lawrencebower.docgen.web_logic.business.mapping.AutoMappedFieldMapper"/>

    <bean name="viewUtils"
          class="org.lawrencebower.docgen.web_logic.business.utils.ViewUtils"/>

</beans>